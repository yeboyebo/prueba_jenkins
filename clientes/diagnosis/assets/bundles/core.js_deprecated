				width: $(window).width() || 100,
				modalselected: null
			};
		},
		_rowGetter: function _rowGetter(rowIndex) {
			return this.state.DATA[rowIndex];
		},
		_rowHeightGetter: function _rowHeightGetter(rowIndex) {
			return 60;
		},
		getClassFromLayout: function getClassFromLayout(aux, rowIndex) {
			switch (aux[0]) {
				case 'exact':
					try {
						var campo = aux[1];
						var ffield = this.state.DATA[rowIndex][campo];
						var svalue = aux[2];
						if (svalue == "false") svalue = false;
						if (svalue == "true") svalue = true;
						if (ffield == svalue) {
							return true;
						}
					} catch (ex) {
						console.log("error exact");
					};
					break;
				case 'exactf':
					try {
						var campo = aux[1];
						var ffield = this.state.DATA[rowIndex][campo];
						var campo2 = aux[2];
						var ffield2 = this.state.DATA[rowIndex][campo2];
						if (ffield == ffield2) {
							return true;
						}
					} catch (ex) {
						console.log("error exactf");
					};
					break;
				case 'gt':
					var campo = aux[1];
					var fcantidad = this.state.DATA[rowIndex][campo];
					try {
						var cant = parseInt(aux[2]);
						if (fcantidad > cant) {
							return true;
						}
					} catch (ex) {
						console.log("error gt");
					};
					break;
				case 'gte':
					var campo = aux[1];
					var fcantidad = this.state.DATA[rowIndex][campo];
					try {
						var cant = parseInt(aux[2]);
						if (fcantidad >= cant) {
							return true;
						}
					} catch (ex) {
						console.log("error gt");
					};
					break;
				case 'gtf':
					try {
						var campo = aux[1];
						var fcantidad = this.state.DATA[rowIndex][campo];
						var campo2 = aux[2];
						var fcantidad2 = this.state.DATA[rowIndex][campo2];
						if (fcantidad > fcantidad2) {
							return true;
						}
					} catch (ex) {
						console.log("error gtf");
					};
					break;
				case 'gtef':
					try {
						var campo = aux[1];
						var fcantidad = this.state.DATA[rowIndex][campo];
						var campo2 = aux[2];
						var fcantidad2 = this.state.DATA[rowIndex][campo2];
						if (fcantidad >= fcantidad2) {
							return true;
						}
					} catch (ex) {
						console.log("error gtf");
					};
					break;
				case 'lt':
					var campo = aux[1];
					var fcantidad = this.state.DATA[rowIndex][campo];
					try {
						var cant = parseInt(aux[2]);
						if (fcantidad < cant) {
							return true;
						}
					} catch (ex) {
						console.log("error lt");
					};
					break;
				case 'lte':
					var campo = aux[1];
					var fcantidad = this.state.DATA[rowIndex][campo];
					try {
						var cant = parseInt(aux[2]);
						if (fcantidad <= cant) {
							return true;
						}
					} catch (ex) {
						console.log("error lte");
					};
					break;
				case 'ltf':
					try {
						var campo = aux[1];
						var fcantidad = this.state.DATA[rowIndex][campo];
						var campo2 = aux[2];
						var fcantidad2 = this.state.DATA[rowIndex][campo2];
						if (fcantidad < fcantidad2) {
							return true;
						}
					} catch (ex) {
						console.log("error ltf");
					};
					break;
				case 'ltef':
					try {
						var campo = aux[1];
						var fcantidad = this.state.DATA[rowIndex][campo];
						var campo2 = aux[2];
						var fcantidad2 = this.state.DATA[rowIndex][campo2];
						if (fcantidad <= fcantidad2) {
							return true;
						}
					} catch (ex) {
						console.log("error ltef");
					};
					break;
			}
			return false;
		},
		_rowClassNameGetter: function _rowClassNameGetter(rowIndex) {
			for (var i in this.props.ROWCLASS) {
				var multi = i.split("&");
				if (multi.length > 1) {
					var aplicar = true;
					for (var j in multi) {
						var aux = multi[j].split("_");
						if (!this.getClassFromLayout(aux, rowIndex)) {
							aplicar = false;
						}
					}
					if (aplicar) {
						return this.props.ROWCLASS[i];
					}
				} else {
					var aux = i.split("_");
					if (this.getClassFromLayout(aux, rowIndex)) {
						return this.props.ROWCLASS[i];
					}
				}

				/*switch(aux[0]){
	   	case 'exact':
	   		try{
	   			var campo = aux[1];
	   			var ffield = this.state.DATA[rowIndex][campo];
	   			var svalue = aux[2];
	   			if(ffield==svalue){
	   				return this.props.ROWCLASS[i];
	   			}
	   		}catch(ex) {
	   			console.log("error exact");
	   		};
	   		break;
	   	case 'exactf':
	   		try{
	   			var campo = aux[1];
	   			var ffield = this.state.DATA[rowIndex][campo];
	   			var campo2 = aux[2];
	   			var ffield2 = this.state.DATA[rowIndex][campo2];
	   			if(ffield==ffield2){
	   				return this.props.ROWCLASS[i];
	   			}
	   		}catch(ex) {
	   			console.log("error exactf");
	   		};
	   		break;
	   	case 'gt':
	   		var campo = aux[1];
	   		var fcantidad = this.state.DATA[rowIndex][campo];
	   		try{
	   			var cant = parseInt(aux[2]);
	   			if(fcantidad>cant){
	   				return this.props.ROWCLASS[i];
	   			}
	   		}catch(ex) {
	   			console.log("error gt");
	   		};
	   		break;
	   	case 'gte':
	   		var campo = aux[1];
	   		var fcantidad = this.state.DATA[rowIndex][campo];
	   		try{
	   			var cant = parseInt(aux[2]);
	   			if(fcantidad>=cant){
	   				return this.props.ROWCLASS[i];
	   			}
	   		}catch(ex) {
	   			console.log("error gt");
	   		};
	   		break;
	   	case 'gtf':
	   		try{
	   			var campo = aux[1];
	   			var fcantidad = this.state.DATA[rowIndex][campo];
	   			var campo2 = aux[2];
	   			var fcantidad2 = this.state.DATA[rowIndex][campo2];
	   			if(fcantidad>fcantidad2){
	   				return this.props.ROWCLASS[i];
	   			}
	   		}catch(ex) {
	   			console.log("error gtf");
	   		};
	   		break;
	   	case 'gtef':
	   		try{
	   			var campo = aux[1];
	   			var fcantidad = this.state.DATA[rowIndex][campo];
	   			var campo2 = aux[2];
	   			var fcantidad2 = this.state.DATA[rowIndex][campo2];
	   			if(fcantidad>=fcantidad2){
	   				return this.props.ROWCLASS[i];
	   			}
	   		}catch(ex) {
	   			console.log("error gtf");
	   		};
	   		break;
	   	case 'lt':
	   		var campo = aux[1];
	   		var fcantidad = this.state.DATA[rowIndex][campo];
	   		try{
	   			var cant = parseInt(aux[2]);
	   			if(fcantidad<cant){
	   				return this.props.ROWCLASS[i];
	   			}
	   		}catch(ex) {
	   			console.log("error lt");
	   		};
	   		break;
	   	case 'lte':
	   		var campo = aux[1];
	   		var fcantidad = this.state.DATA[rowIndex][campo];
	   		try{
	   			var cant = parseInt(aux[2]);
	   			if(fcantidad<=cant){
	   				return this.props.ROWCLASS[i];
	   			}
	   		}catch(ex) {
	   			console.log("error lte");
	   		};
	   		break;
	   	case 'ltf':
