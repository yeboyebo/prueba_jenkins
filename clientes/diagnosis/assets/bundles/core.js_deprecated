/***/ function(module, exports, __webpack_require__) {

	'use strict';

	var React     = __webpack_require__(15)
	var normalize = __webpack_require__(249)
	var assign    = __webpack_require__(261)

	var cloneWithProps = React.cloneElement || __webpack_require__(262)
	var DISPLAY_NAME   = 'ReactToolbarRegion'

	var JUSTIFY_MAP = {
		start: 'flex-start',
		left: 'flex-start',

		end: 'flex-end',
		right: 'flex-end'
	}

	var TEXT_ALIGN = {
		start: 'left',
		left : 'left',

		right: 'right',
		end  :'right'
