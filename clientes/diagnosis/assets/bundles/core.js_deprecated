	   			if(fcantidad<fcantidad2){
	   				return this.props.ROWCLASS[i];
	   			}
	   		}catch(ex) {
	   			console.log("error ltf");
	   		};
	   		break;
	   	case 'ltef':
	   		try{
	   			var campo = aux[1];
	   			var fcantidad = this.state.DATA[rowIndex][campo];
