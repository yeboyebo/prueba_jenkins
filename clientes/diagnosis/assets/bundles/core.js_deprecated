/* 1 */
/***/ function(module, exports, __webpack_require__) {

	//Fichero principal para realizar extensiones
	'use strict';

	module.exports = {
	  URLResolver: __webpack_require__(2),
	  helpers: __webpack_require__(3)
	};
	//Extensiones

/***/ },
/* 2 */
/***/ function(module, exports) {

	/*
	Este modulo sera responsable de procesar peticiones de accionpermitira a la parte Javascrip resolver URL estandar
	de  REST...
	No se conecta a servidor, sino que tendra que mantenerse configuracion coherente con los router
	Es un singleton
	*/

	//Atributos internos
	'use strict';

	var _directorioRaiz = '{%url "root"%}';

	// Constructor
	function URLResolver() {};

	// class methods
	function getBaseREST(aplic, modelo, id, tipo) {
	    id = id || 0;
	    if (id) {
	        return _directorioRaiz + aplic + '/REST/' + modelo + '/' + id + '/' + tipo;
	    } else {
	        return _directorioRaiz + aplic + '/REST/' + modelo + '/' + tipo;
	    }
	};

	function getBaseLayOut(aplic, modelo, id, isQuery) {
	    id = id || 0;
	    if (id) {
	        return _directorioRaiz + aplic + '/' + modelo + '/PK/' + id;
	    } else {
	        if (isQuery) {
	            return _directorioRaiz + aplic + '/' + modelo + '/Q';
	        } else {
	            return _directorioRaiz + aplic + '/' + modelo;
	        }
	    }
	};

	//Establecer directorio base
	URLResolver.prototype.setRaiz = function (raiz) {
	    //TO DO:Comprobar acaba en /
	    _directorioRaiz = raiz;
	};

	URLResolver.prototype.getREST = function (aplic, modelo, id) {
	    return getBaseREST(aplic, modelo, id, 'detail');
	};

	URLResolver.prototype.getRESTAccion = function (aplic, modelo, id, accion) {
	    return getBaseREST(aplic, modelo, id, 'accion') + '/' + accion;
	};

	URLResolver.prototype.getRESTQuery = function (aplic, modelo, busq) {
	    busq = busq || 'list';
	    return getBaseREST(aplic, modelo, 0, busq);
	};

	URLResolver.prototype.getLayOUT = function (aplic, modelo, template, id, modif, isQuery) {
	    isQuery = isQuery || false;
	    modif = modif || '';
	    var aux = getBaseLayOut(aplic, modelo, id, isQuery) + '/' + template;
	    if (modif != '') {
	        aux += '/' + modif;
	    };
	    return aux;
	};
	// export the Object

	module.exports = new URLResolver();

/***/ },
/* 3 */
/***/ function(module, exports, __webpack_require__) {

	/*
	Funciones rapidas para ejecutar metodos
	*/
	"use strict";

	var $ = __webpack_require__(4);
	//Inicializacion para que haga stringify
	$.ajaxSetup({
		beforeSend: function beforeSend(jqXHR, options) {
			if (options.contentType.indexOf("application/json") >= 0 && typeof options.data != "string") {
				options.data = JSON.stringify(options.data);
			}
		}
	});
	// Carga AJAX (onEnd no requerido)
	module.exports.loadAJAX = function (selector, url, onEnd) {
		var $aux = $(selector);
		$aux.load(url, onEnd);
		return $aux;
	};

	module.exports.getAJAX = function (url) {
		var midata;
		$.ajax({
			url: url,
			async: false,
			dataType: 'json',
			success: function success(data) {
				midata = data;
			},
			error: function error(xhr) {
				midata = null;
			}
		});
		return midata;
	};

	var _default_error = function _default_error(xhr, textStatus, errorThrown) {
		alert('error en peticion:' + textStatus);
		console.log('PETicion error');
		console.log(xhr.status + ": " + xhr.responseText);
	};
	var _default_success = function _default_success(data) {
		console.log('PETICION CORRECTA');
		console.log(data);
	};

	module.exports.resquestAccion = function (url, data, method, misuccess, mierror) {
		method = method || 'PUT';
		misuccess = misuccess || _default_success;
		mierror = mierror || _default_error;
		return $.ajax({
			url: url,
			data: data,
			dataType: 'json',
			error: mierror,
			success: misuccess,
			contentType: 'application/json',
			method: method,
			processData: false,
			async: false
		});
	};

	module.exports.resquestAccionDelete = function (url, data, method, misuccess, mierror) {
		method = method || 'PUT';
		misuccess = misuccess || _default_success;
		mierror = mierror || _default_error;
		$.ajax({
			url: url,
			data: data,
			dataType: 'json',
			error: mierror,
			contentType: 'application/json',
			success: misuccess,
			method: method,
			processData: false
		});
	};

	module.exports.resquestGET = function (url, QueryParam, misuccess, mierror) {
		var method = 'GET';
		QueryParam = QueryParam || {};
		var aux = $.param(QueryParam);
		var miurl = url + '?' + aux;
		//console.log('URL GET : ' + miurl);
		misuccess = misuccess || _default_success;
		mierror = mierror || _default_error;
		$.ajax({
			url: miurl,
			dataType: 'json',
			error: mierror,
			success: misuccess,
			method: method
		});
	};

	module.exports.resquestGETre = function (url, QueryParam, misuccess, mierror) {
		var method = 'GET';
		QueryParam = QueryParam || {};
		var aux = $.param(QueryParam);
		var miurl = url + '?' + aux;
		//console.log('URL GET : ' + miurl);
		misuccess = misuccess || _default_success;
		mierror = mierror || _default_error;
		return $.ajax({
			url: miurl,
			async: false,
			dataType: 'json',
			error: mierror,
			success: misuccess,
			method: method
		});
	};

	module.exports.emptyf = function () {
		return null;
	};

/***/ },
