			this.setState({ val: valor });
		},

		onChange: function onChange(event, v) {
			var f = this.props.modelfield;
			if (f.option_list && f.option_list.tipo == 2) {
				this.setState({ val_auto: v.newValue });
			} else {
				var valor = this.refs.fieldComp.value;
				//console.log("change " + valor);
				if (this.state.DATA[0]) {
					this.state.DATA[0][this.props.modelfield.key] = valor;
				} else {
					this.state.DATA[this.props.modelfield.key] = valor;
				}
				this.setState({ DATA: this.state.DATA });
				if (this.props.onChange) this.props.onChange(this.refs.fieldComp.name, this.refs.fieldComp.value);else this.lanzarAccion('changed', valor);
			}
		},

