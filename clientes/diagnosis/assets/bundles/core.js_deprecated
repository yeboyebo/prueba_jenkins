	/*
	  TableGrid basada en http://allenfang.github.io/react-bootstrap-table/example.html
	  Las propiedades que recibe son:
		-COLUMNS: Lista de objeto columns, con las siguientes propiedades:		
						   -dataField={keym} 
	                       -isKey={keym=='pk' ? true : false} 
	                       -dataAlign={align}
	                       -width={150}
	                       -hidden={hidden}
		-PAG:True/False
		-SELECT: 
			- 0 No
			- 1 Simple
			- 2 Multiple
		-HEIGHT
		-FUNCIONES DE EVENTOS
	   El estado que recibe:
		-DATA:Lista de valores
		-SELECTED:
		-HASMORE:True/False
		-COUNT: Numero de registros
	   Eventos:
		-RowSelected.
		-MasDatos.

	*/
	'use strict';

	var _extends = Object.assign || function (target) { for (var i = 1; i < arguments.length; i++) { var source = arguments[i]; for (var key in source) { if (Object.prototype.hasOwnProperty.call(source, key)) { target[key] = source[key]; } } } return target; };

	var React = __webpack_require__(15);
	var tableBootstrap = __webpack_require__(174);
	var _ = __webpack_require__(9);
	var comp_mixins = __webpack_require__(175);
	var formatter = __webpack_require__(12);
	var ReactDOM = __webpack_require__(172);

	var tableGridBase = {
		propTypes: {
			COLUMNS: React.PropTypes.array.isRequired,
			SELECT: React.PropTypes.number.isRequired,
			PAG: React.PropTypes.bool,
			HEIGHT: React.PropTypes.string,
			ONSELECTED: React.PropTypes.func,
			ONSELECTEDALL: React.PropTypes.func,
			ONMASDATOS: React.PropTypes.func,
			ONCOUNT: React.PropTypes.func
		},
		ONSELECTED: function ONSELECTED(row, isSelected) {
			this.props.ONSELECTED(row, isSelected);
		},
		ONSELECTEDALL: function ONSELECTEDALL(event) {
			this.props.ONSELECTEDALL();
		},
		ONMASDATOS: function ONMASDATOS(event) {
			this.props.ONSMASDATOS();
		},
		ONCOUNT: function ONCOUNT(event) {
			this.props.ONCOUNT();
		},
		getDefaultProps: function getDefaultProps() {
			return {
				HEIGHT: "400",
				PAG: true
			};
		},
		getInitialState: function getInitialState() {
			return {
				DATA: [],
				HASMORE: false,
				COUNT: null
			};
		},

		render: function render() {
			var selectRowProp = {};
			if (this.props.SELECT > 0) {
				selectRowProp = {
					mode: this.props.SELECT == 1 ? "radio" : "checkbox",
					clickToSelect: true,
					bgColor: "rgb(238, 193, 213)",
					onSelect: this.ONSELECTED,
					onSelectAll: this.ONSELECTEDALL
				};
			};
			var micolumns = [];
			_.each(this.props.COLUMNS, function (obj) {
				micolumns.push(React.createElement(
					tableBootstrap.TableHeaderColumn,
					obj,
					obj.verbose_name
				));
			});
			var paginacion = '';

			if (this.props.PAG) {
				var elementos = this.state.DATA.length;
				var auxCont = this.state.COUNT == null ? 'de ...' : ' de ' + this.state.COUNT.toString();
				paginacion = React.createElement(
					'ul',
					{ className: 'pagination' },
					React.createElement(
						'span',
						{ className: 'disabled' },
						' 1 a ',
						elementos,
						' ',
						auxCont,
						' '
					),
					React.createElement(
						'li',
						{ className: this.state.HASMORE ? null : 'disabled' },
						React.createElement(
							'a',
							{ onClick: this.ONMASDATOS },
							'»'
						)
					),
					React.createElement(
						'li',
						null,
						React.createElement(
							'a',
							{ className: this.state.COUNT ? 'disabled' : null, onClick: this.ONCOUNT },
							'?'
						)
					)
				);
			};
			return React.createElement(
				'div',
				null,
				React.createElement(
					tableBootstrap.BootstrapTable,
					{ height: 400,
						data: this.state.DATA,
						striped: true,
						hover: true,
						selectRow: selectRowProp },
					micolumns
				),
				paginacion
			);
		}
	};

	var tableGridStore = _.extend(_.clone(tableGridBase), {
		mixins: [comp_mixins.getStateLE],
		propTypes: {
			COLUMNS: React.PropTypes.array.isRequired,
			SELECT: React.PropTypes.number.isRequired,
			PAG: React.PropTypes.bool,
			HEIGHT: React.PropTypes.string
		},
		ONSELECTED: function ONSELECTED(row, isSelected) {
			if (isSelected) {
				this.lanzarAccion("selected", [row.pk]);
			} else {
				this.lanzarAccion("selected", []);
			};
		},
		ONSELECTEDALL: function ONSELECTEDALL(event) {
			this.lanzarAccion("selected", []);
		},
		ONMASDATOS: function ONMASDATOS(event) {
			if (this.state.HASMORE) {
				this.lanzarAccion('masData', {});
			};
		},
		ONCOUNT: function ONCOUNT(event) {
			if (this.state.HASMORE) {
				this.lanzarAccion('count', {});
			};
		}
	});

	var TableGrid1 = React.createClass(tableGridBase);
	var TableGrid2 = React.createClass(tableGridStore);

	function _getColumnsFromSCHEMALAYOUT(SCHEMA, LAYOUT) {
		//TODO, AQUI ES DONDE TIENE QUE DEDUCIRSE A PARTIR DE LAYOUT Y SCHEMA mas propiedades, formateo...
		var micolumns = [];
		var align;
		var daFormato;
