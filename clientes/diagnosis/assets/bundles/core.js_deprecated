		_rendercomp: function _rendercomp() {
			var f = this.props.modelfield;
			var val = null;
			var disabled = false;
			/*if(this.props.modeldata != this.state.val)
	  	val = this.props.modeldata;
	  else
	  	val = this.state.val;
	  val = val == null ? (f.default != null ? f.default : null) : val;*/
			if (this.state.DATA) {
				if (this.state.DATA[0]) {
					val = this.state.DATA[0][this.props.modelfield.key];
				} else {
					val = this.state.DATA[this.props.modelfield.key];
				}
			}
			if (this.props.jsonmeta) {
				disabled = this.props.jsonmeta.disabled ? true : false;
			}
			if (f.option_list) {
				val = val ? val : f.option_list.VALUE;
				if (f.option_list.tipo == 1) {
					var items = _.map(f.option_list.LIST, function (item, index) {
						return React.createElement(
							'option',
							{
								key: index,
								value: index },
							item
						);
					});
					return React.createElement(
						'select',
						{
							ref: 'fieldComp',
							id: f.name,
							name: this.props.modelfield.key,
							value: this.state.val,
							onChange: this._onSelected,
							onBlur: this.onBlur,
							onFocus: this.onFocus },
						items
					);
				} else {
					// Relacionado
					var modelKeyAuto = this.props.modelfield.key + "_auto";
					var nameAuto = f.name + "_auto";
					//var suggestions = this.getSuggestions();
					var suggestions = this.state.suggestions;
					var inputProps = {
						value: this.state.val_auto,
						onKeyDown: this.onKeyDown,
						onChange: this.onChange,
						type: 'search',
						id: modelKeyAuto,
						placeholder: 'Busca valor'
					};
					return React.createElement(
						'div',
						null,
						React.createElement(Autosuggest, {
							classname: 'autosug',
							name: nameAuto,
							ref: 'autocomplete',
							suggestions: suggestions,
							onSuggestionSelected: this.onSuggestionSelected,
							onSuggestionsUpdateRequested: this.onSuggestionsUpdateRequested,
							getSuggestionValue: this.getSuggestionValue,
							renderSuggestion: this.renderSuggestion,
							inputProps: inputProps,
							className: 'form-control',
							focusInputOnSuggestionClick: false }),
						React.createElement('input', {
							key: this.props.modelfield.key,
							ref: 'fieldComp',
							id: 'autocomp',
							name: this.props.modelfield.key,
							className: 'form-control floatinginput react-autosuggest__container',
							autoFocus: true,
							type: 'text',
							value: val,
							onChange: this.onChange,
							disabled: 'disabled' })
					);
				}
			} else if (f.subtipo == 6) {
				// TextArea
				return React.createElement('textarea', { className: 'form-control',
					rows: '2',
					id: f.name,
					ref: 'fieldComp',
					name: this.props.modelfield.key,
					disabled: disabled,
					value: val,
					onChange: this.onChange,
					onBlur: this.onBlur,
					onFocus: this.onFocus });
			} else if (f.tipo == 3) {
				// String
				return React.createElement('input', {
					key: this.props.modelfield.key,
					ref: 'fieldComp',
					id: f.name,
					name: this.props.modelfield.key,
					className: 'form-control',
					type: 'text',
					value: val,
					disabled: disabled,
					onChange: this.onChange,
					onBlur: this.onBlur,
					onFocus: this.onFocus });
			} else if (f.tipo == 16 || f.tipo == 19 || f.tipo == 37) {
				// Number / Currency
				// 37 == CURRENCY
				return React.createElement('input', {
					ref: 'fieldComp',
					id: f.name,
					name: this.props.modelfield.key,
					className: 'form-control numberField',
					type: 'number',
					value: val,
					pattern: '[0-9]',
					disabled: disabled,
					onChange: this.onChange,
					onBlur: this.onBlur,
					onFocus: this.onFocus });
			} else if (f.tipo == 18) {
				// Boolean
				return React.createElement('input', {
					ref: 'fieldComp',
					id: f.name,
					name: this.props.modelfield.key,
					type: 'checkbox',
					checked: val,
					disabled: disabled,
					onChange: this.onChecked,
					onBlur: this.onBlur,
					onFocus: this.onFocus });
			} else if (f.tipo == 26) {
				// Date
				return React.createElement('input', {
					ref: 'fieldComp',
					id: f.name,
					name: this.props.modelfield.key,
					className: 'form-control',
					type: 'date',
					value: val,
					disabled: disabled,
					onChange: this.onChange,
					onBlur: this.onBlur,
					onFocus: this.onFocus });
			} else {
				// Default
				return React.createElement('input', {
					ref: 'fieldComp',
					id: f.name,
					name: this.props.modelfield.key,
					className: 'form-control',
					type: 'text',
					value: val,
					disabled: disabled,
					onChange: this.onChange,
					onBlur: this.onBlur,
					onFocus: this.onFocus });
			}
		},

		render: function render() {
			var rendercomp = this._rendercomp();
			var f = this.props.modelfield;
			var className = f.hasOwnProperty("visible") && !f.visible ? "oculto" : "";
