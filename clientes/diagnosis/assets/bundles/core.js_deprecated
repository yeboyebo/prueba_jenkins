				this.state.DATA[this.props.modelfield.key] = valor;
			}
			this.setState({ DATA: this.state.DATA });
			if (this.props.onChange) this.props.onChange(this.refs.fieldComp.name, valor);else this.lanzarAccion('changed', valor);
		}
	};

