		},

		_pintaAcciones: function _pintaAcciones(e, key, data, index) {
			var acciones = [];
			for (var a in this.props.ACCIONES) {
				if (this.props.ACCIONES[a] == "delete") {
					acciones.push(React.createElement(
						'i',
						{ key: this.props.ACCIONES[a], className: 'material-icons', onClick: this._onModal.bind(this, key, data, index) },
						'delete'
					));
				} else {
					acciones.push(React.createElement(
						'i',
						{ key: this.props.ACCIONES[a].nombre, className: 'material-icons', onClick: this._onAccionPress.bind(this, data, a) },
						this.props.ACCIONES[a].icono
					));
				}
			}
			return React.createElement(
				'div',
				{ className: 'accion' },
				acciones
			);
		},

		_onAccionPress: function _onAccionPress(that, accion, e) {
			var midata = this.props.ACCIONES[accion];
			midata['pk'] = that.data.pk;
			this.lanzarAccion('accionActivated', midata);
		},

		_onDelete: function _onDelete() {
			this.cierraModal();
			this.lanzarAccion('deletefila', this.state.modalselected);
		},

		_onModal: function _onModal(key, data, index, e) {
			this.cierraModal();
			this.setState({ modalselected: key });
		},

		_onSelected: function _onSelected(id, data, e) {
			if (this.props.ACCIONES && (e.target.getAttribute('name') == 'Ac' || e.target.classList[0] == 'material-icons')) {} else {
				if (e.target.type == 'checkbox') {
					console.log("chekbox");
				} else {
					this.ONSELECTED(data);
				}
			}
		},

		cierraModal: function cierraModal() {
			$(".modalContent,.fondogris").toggleClass("active");
		},

		updateDimensions: function updateDimensions() {
			var w = window,
			    d = document,
			    documentElement = d.documentElement,
			    body = d.getElementsByTagName('body')[0],
			    width = w.innerWidth || documentElement.clientWidth || body.clientWidth,
			    height = w.innerHeight || documentElement.clientHeight || body.clientHeight;
			if ($("#" + this.props.ID).innerWidth() == 0) {
				if (width > 600) {
					width = width - 200;
				} else {
					width = width - 60;
				}
			}
			width = width - 60;

			this.setState({ width: width || 100 });
		},
		componentWillMount: function componentWillMount() {
			this.updateDimensions();
		},
		componentDidMount: function componentDidMount() {
			window.addEventListener("resize", this.updateDimensions);
		},
		componentWillUnmount: function componentWillUnmount() {
			window.removeEventListener("resize", this.updateDimensions);
		},
		onColumnResize: function onColumnResize(firstCol, firstSize, secondCol, secondSize) {
			firstCol.width = firstSize;
			this.setState({});
		},
		render: function render() {
			var este = this;
			var micolumns = this.props.COLUMNS;

			if (this.props.TIPO == "multicheck") {
				micolumns[0].hidden = false;
				micolumns[0].render = function (value, data, rowData, index) {
					return React.createElement('input', { type: 'checkbox', value: data.checked, onChange: este._onChecked.bind(null, data) });
				};
			}

			if (this.props.ACCIONES) {
				var len = micolumns.length - 1;
				micolumns[len].hidden = false;
				micolumns[len].render = este._pintaAcciones;
				//micolumns[micolumns.length-1].render(este._pintaAcciones);
			}
			var buscador = [];
			if (this.props.TIPO == "search") {
				buscador.push(React.createElement(
					'div',
					{ className: 'buscador', key: 'buscar' },
					React.createElement(
						'label',
						{ className: 'col-sm', htmlFor: this.props.modelkey },
						'Buscar'
					),
					React.createElement(
						'div',
						{ className: 'col-sm' },
						React.createElement('input', {
							type: 'text',
							className: 'form-control',
							name: 'buscador',
							ref: 'buscador',
							id: 'buscador',
							onKeyPress: this._onKeyPress })
					)
				));
			}

			var paginacion = '';

			if (this.props.PAG) {
				var elementos = this.state.DATA.length;
				var pagina = parseInt(this.state.PO / this.state.PL);
				var po = pagina == 0 ? 1 : pagina;
				var totalpags = this.state.COUNT == null ? 0 : parseInt(this.state.COUNT / this.state.PL) + 1;
				var auxCont = this.state.COUNT == null ? 'de ...' : ' de ' + totalpags.toString();
				paginacion = React.createElement(
					'ul',
					{ className: 'paginacion' },
					React.createElement(
						'li',
						null,
						React.createElement(
							'a',
							{ onClick: this.ONPREVIOUSPAGE },
							'«'
						),
						' ',
						po,
						' ',
						auxCont,
						' ',
						React.createElement(
							'a',
							{ onClick: this.ONNEXTPAGE },
							'»'
						)
					)
				);
			};

			var data = this.state.DATA || [];
			var eliminar = "";
			if (this.state.modalselected) {
				eliminar = this.state.modalselected.descripcion || this.state.modalselected.nombrecliente;
			}
			return React.createElement(
				'div',
				{ className: 'tablegrid' },
				React.createElement('div', { className: 'fondogris' }),
				React.createElement(
					'div',
					{ className: 'modalContent' },
					React.createElement(
						'div',
						{ className: 'modalCuerpo' },
						'La línea ',
						React.createElement(
							'b',
							null,
							eliminar
						),
						' sera eliminada'
					),
					React.createElement(
						'div',
						{ className: 'modalPie' },
						React.createElement(
							'a',
							{ href: 'javascript:void(0)', className: 'btn btn-primary btn-submit modal-btn', onClick: this._onDelete },
							'Confirmar'
						),
						React.createElement(
							'a',
							{ href: 'javascript:void(0)', className: 'btn btn-primary btn-submit modal-btn', onClick: this.cierraModal },
							'Cancelar'
						)
					)
				),
				buscador,
				React.createElement(DataGrid, {
					idProperty: 'id',
					dataSource: data,
					columns: micolumns,
					selected: this.state.rowSelected,
					onColumnResize: this.onColumnResize,
					onSelectionChange: this._onSelected,
					rowStyle: this.rowStyle }),
				paginacion
			);
		},
		_onChecked: function _onChecked(data, e, b) {
			console.log("----");
			console.log(data);
			console.log(e.target.checked);
			console.log(b);
			console.log("----");
		},
		rowStyle: function rowStyle(data, props) {
			var style = {};
			/*if (props.index % 4 == 0){
	  	style.color = 'blue'
	  }
	  	if (data.country == 'USA'){
	  	style.background = '#FFD3D3'
	  	style.fontWeight = 'bold'
	  }*/

			return style;
		}
	};

	var tableGridStore = _.extend(_.clone(tableGridBase), {
		mixins: [comp_mixins.getStateLE],
		propTypes: {
			COLUMNS: React.PropTypes.array.isRequired,
			SELECT: React.PropTypes.number.isRequired,
