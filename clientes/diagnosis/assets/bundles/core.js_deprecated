/* 14 */
/***/ function(module, exports, __webpack_require__) {

	/* WEBPACK VAR INJECTION */(function($) {/*
	  Tabla basada en https://www.npmjs.com/package/react-navbar
	  Se hace coherente con la definicion de tipos JSON menus. Listas de objetos con:
	   - Deben de tener un campo TEXT
	   - Deben de tener un TIPO: 
			TIPO 1- Submenus, en DATA deben incluir lista de submenus.
			TIPO 2- Separadores
			(de principio no presupone de nada mas).
			A la funcion de callBack se le pasa el objeto entero menu para que lo procese.

	*/
	'use strict';

	var React = __webpack_require__(15);
	var ReactDOM = __webpack_require__(172);

	function classNames() {

	    var classes = '';

	    for (var i = 0; i < arguments.length; i++) {
	        var arg = arguments[i];
	        if (!arg) continue;

	        var argType = typeof arg;

	        if ('string' === argType || 'number' === argType) {
	            classes += ' ' + arg;
	        } else if (Array.isArray(arg)) {
	            classes += ' ' + classNames.apply(null, arg);
	        } else if ('object' === argType) {
	            for (var key in arg) {
	                if (arg.hasOwnProperty(key) && arg[key]) {
	                    classes += ' ' + key;
	                }
	            }
	        }
	    }

	    return classes.substr(1);
	};

	var Item = React.createClass({
	    displayName: 'Item',

	    propTypes: {
	        item: React.PropTypes.object.isRequired,
	        func: React.PropTypes.func.isRequired
	    },

	    selected: function selected(event) {
	        this.props.func(this.props.item);
	    },
	    render: function render() {
	        var selected = this.selected;
	        if ('USER' in this.props.item) {
	            if (this.props.item.USER === null) {
	                selected = function () {
	                    //alert("selecciona un usuario");
	                    $.toaster({ message: "Debes sellecionar un usuario", template: "navbar" });
	                };
	            }
	        }
	        if (this.props.item.TIPO == 2) {
	            return React.createElement('li', { role: 'separator', className: 'divider' });
	        } else {
	            return React.createElement(
	                'li',
	                { className: 'menuElement' },
	                React.createElement(
	                    'a',
	                    { href: '#', className: 'menuUrl', onClick: selected },
	                    this.props.item.TEXT
	                )
	            );
	        };
	    }
	});

	var Dropdown = React.createClass({
	    displayName: 'Dropdown',

	    propTypes: {
	        title: React.PropTypes.string.isRequired,
	        items: React.PropTypes.array.isRequired,
	        func: React.PropTypes.func.isRequired
	    },

	    render: function render() {
	        var _this = this;

	        var items = this.props.items.map(function (item, index) {
	            var ref = 'i' + index;
	            return React.createElement(Item, { item: item, ref: ref, key: ref, func: _this.props.func });
	        });

	        return React.createElement(
	            'li',
	            { className: 'dropdown' },
	            React.createElement(
	                'a',
	                { href: '#', className: 'dropdown-toggle', 'data-toggle': 'dropdown', role: 'button', 'aria-haspopup': 'true', 'aria-expanded': 'false' },
	                this.props.title,
	                React.createElement('span', { className: 'caret' })
	            ),
	            React.createElement(
	                'ul',
	                { className: 'dropdown-menu' },
	                items
	            )
	        );
	    }
	});

	var MenuItems = React.createClass({
	    displayName: 'MenuItems',

	    propTypes: {
	        items: React.PropTypes.array.isRequired,
	        func: React.PropTypes.func.isRequired,
	        secondary: React.PropTypes.bool
	    },

	    render: function render() {
	        var _this2 = this;

	        var className = classNames('nav navbar-nav', { 'navbar-right': this.props.secondary });

	        var items = this.props.items.map(function (item, index) {
	            return _this2.itemElement(item, 'i' + index);
	        });
	        return React.createElement(
	            'ul',
	            { className: className },
	            items
	        );
	    },

	    itemElement: function itemElement(item, ref) {
	        return item.TIPO == 1 ? React.createElement(Dropdown, { title: item.TEXT, items: item.DATA, ref: ref, key: ref, func: this.props.func }) : React.createElement(Item, { item: item, ref: ref, key: ref, func: this.props.func });
	    }
	});

	var NavBar = React.createClass({
	    displayName: 'NavBar',

	    propTypes: {
	        title: React.PropTypes.string.isRequired,
	        menuItems: React.PropTypes.array.isRequired,
	        mifunc: React.PropTypes.func.isRequired,
	        secondaryMenuItems: React.PropTypes.array

	    },

	    render: function render() {
	        var logo = this.props.urlStatic + "logo-v.png";
	        var secondaryMenuItems = this.props.secondaryMenuItems ? React.createElement(MenuItems, {
	            ref: 'secondaryMenuItems',
	            secondary: true,
	            items: this.props.secondaryMenuItems,
	            func: this.props.mifunc }) : null;

	        return React.createElement(
	            'nav',
	            { className: 'navbar navbar-default' },
	            React.createElement(
	                'div',
	                { className: 'container' },
	                React.createElement(
	                    'div',
	                    { className: 'navbar-header' },
	                    React.createElement(
	                        'button',
	                        { type: 'button', className: 'navbar-toggle collapsed', 'data-toggle': 'collapse', 'data-target': '#react-navbar-collapse-all', 'aria-expanded': 'false' },
	                        React.createElement(
	                            'span',
	                            { className: 'sr-only' },
	                            'Toggle navigation'
	                        ),
	                        React.createElement('span', { className: 'icon-bar' }),
	                        React.createElement('span', { className: 'icon-bar' }),
	                        React.createElement('span', { className: 'icon-bar' })
	                    ),
	                    React.createElement(
	                        'div',
	                        { className: 'navbar-user' },
	                        this.props.user
	                    ),
	                    React.createElement(
	                        'div',
	                        { className: 'navbar-img' },
	                        React.createElement('img', { src: logo, className: 'logo logoTienda' })
	                    )
	                ),
	                React.createElement(
	                    'div',
	                    { className: 'collapse navbar-collapse', id: 'react-navbar-collapse-all' },
	                    React.createElement(MenuItems, { ref: 'menuItems', items: this.props.menuItems, func: this.props.mifunc }),
	                    secondaryMenuItems
	                )
	            )
	        );
	    }
	});

	module.exports.generaYBNavBar = function (domObj, user, titulo, staticurl, primary, func, secondary) {
	    secondary = secondary || null;
	    return ReactDOM.render(React.createElement(NavBar, {
	        title: titulo,
	        menuItems: primary,
	        mifunc: func,
	        urlStatic: staticurl,
	        user: user,
	        secondaryMenuItems: secondary }), domObj);
	};
	/* WEBPACK VAR INJECTION */}.call(exports, __webpack_require__(4)))

