	   			var fcantidad2 = this.state.DATA[rowIndex][campo2];
	   			if(fcantidad<=fcantidad2){
	   				return this.props.ROWCLASS[i];
	   			}
	   		}catch(ex) {
	   			console.log("error ltef");
	   		};
	   		break;
	   }*/
			}
			return null;
			//Retorna estilos
		},

		_cargaCheckbox: function _cargaCheckbox(cellData, cellDataKey, rowData, index) {
			return React.createElement('input', { key: index, type: 'checkbox', value: 'valor', checked: cellData, onChange: this._onSelectChange.bind(null, index) });
		},

		_onRowClick: function _onRowClick(e, index) {
			if (this.props.ACCIONES && (e.target.classList[0] == 'accion' || e.target.classList[0] == 'material-icons')) {} else {
				if (e.target.type == 'checkbox') {
					var rows = this.state.DATA.slice();
					var row = rows[index];
					row.selected = !row.selected;
					rows[index] = row;
					if (row.selected) {
						this.state.SELECTEDS.push(rows[index].pk);
					} else {
						this.state.SELECTEDS.pop(rows[index].pk);
					}
					this.setState({ DATA: rows, SELECTEDS: this.state.SELECTEDS });
					this.ONCHECK(this.state.DATA[index], row.selected, this.state.SELECTEDS);
				} else {
					this.ONSELECTED(this.state.DATA[index]);
				}
			}
		},
		_onKeyPress: function _onKeyPress(e, index) {
			if (e.which == 13) {
				var oBusqueda = {};
				oBusqueda['data'] = this.refs.buscador.value;
				oBusqueda['columns'] = this.props.COLUMNS;
				//this.refs.buscador.blur();
				this.lanzarAccion('busqueda', oBusqueda);
			}
		},
		_handleDblClick: function _handleDblClick(e, index) {
			this.ONDBLCLICK(this.state.DATA[index]);
		},

		/*_handleDblClick:function(e,index){
	 if(e.target.type=='checkbox'){
	 	this._onRowClick(e,index)
	 }else{
	 	this.ONDBLCLICK(this.state.DATA[index]);
	 }
	 //this.ONDBLCLICK(this.state.DATA[index]);
	 },*/

		_accionDelete: function _accionDelete(e, key, data, index) {
			return React.createElement(
				'div',
				{ className: 'accion' },
				React.createElement(
					'i',
					{ className: 'material-icons', onClick: this._onModal.bind(this, key, data, index) },
					'delete'
				)
			);
		},

		_pintaAcciones: function _pintaAcciones(e, key, data, index) {
			var acciones = [];
			for (var a in this.props.ACCIONES) {
				if (this.props.ACCIONES[a] == "delete") {
					acciones.push(React.createElement(
						'i',
						{ key: this.props.ACCIONES[a], className: 'material-icons', onClick: this._onModal.bind(this, key, data, index) },
						'delete'
					));
				} else {
					acciones.push(React.createElement(
						'i',
						{ key: this.props.ACCIONES[a].nombre, className: 'material-icons', onClick: this._onAccionPress.bind(this, data, a) },
						this.props.ACCIONES[a].icono
					));
				}
			}
			return React.createElement(
				'div',
				{ className: 'accion' },
				acciones
			);
		},
		/*_dameRelated:function(e,key,data,index){
	 	var rel = this.props.RELATED[0].split("__");
	 	//console.log(this.state.related);
	 	//var valor = YBAccionList.dameDataRelated(data[rel[0]],rel[1],this.props.IDENT.APLIC,this.props.IDENT.PREFIX);
	 	this.lanzarAccion("dameRelated",this.props.RELATED);
	 	var aux = 2;
	 	return (
	 		<div className="related">
	 			{aux}
	 		</div>
	 	)
	 },*/

		_onAccionPress: function _onAccionPress(data, accion) {
			var midata = this.props.ACCIONES[accion];
			midata['pk'] = data.pk;
			this.lanzarAccion('accionActivated', midata);
		},

		_onDelete: function _onDelete() {
			this.cierraModal();
			this.lanzarAccion('deletefila', this.state.modalselected);
		},

		_onModal: function _onModal(key, data, index, e) {
			this.cierraModal();
			this.setState({ modalselected: data });
		},

		cierraModal: function cierraModal() {
			$(".modalContent,.fondogris").toggleClass("active");
		},

		updateDimensions: function updateDimensions() {
			var w = window,
			    d = document,
			    documentElement = d.documentElement,
			    body = d.getElementsByTagName('body')[0],
			    width = w.innerWidth || documentElement.clientWidth || body.clientWidth,
			    height = w.innerHeight || documentElement.clientHeight || body.clientHeight;
			if ($("#" + this.props.ID).innerWidth() == 0) {
				if (width > 600) {
					width = width - 250;
				} else {
					width = width - 80;
				}
			}
			width = width - 19;
			this.setState({ width: width || 100 });
		},
		componentWillMount: function componentWillMount() {
			this.updateDimensions();
		},
		componentDidMount: function componentDidMount() {
			window.addEventListener("resize", this.updateDimensions);
		},
		componentWillUnmount: function componentWillUnmount() {
			window.removeEventListener("resize", this.updateDimensions);
		},

		render: function render() {
			var width = this.state.width || $(window).width() - 0;
			if (width < parseInt(this.props.WIDTH)) {
				width = this.props.WIDTH;
				if (this.props.ACCIONES) {
					width += 100;
				}
			}
			var selectRowProp = {};

			if (this.props.SELECT > 0) {
				selectRowProp = {
					mode: this.props.SELECT == 1 ? "radio" : "checkbox",
					clickToSelect: true,
					bgColor: "rgb(238, 193, 213)",
					onSelect: this.ONSELECTED,
					onSelectAll: this.ONSELECTEDALL
				};
			};

			var micolumns = [];
			_.each(this.props.COLUMNS, function (obj, i) {
				micolumns.push(React.createElement(tableFace.Column, _extends({
					key: i
				}, obj)));
			});

			if (this.props.ACCIONES) {
				var acwidth = 1 + this.props.ACCIONES.length * 0.5;
				acwidth = 40 * acwidth;
				//width += acwidth;
				micolumns.push(React.createElement(tableFace.Column, {
					key: 'acciones',
					label: '',
					width: acwidth,
					dataKey: 'acciones',
					align: 'center',
					className: 'acciones',
					cellRenderer: this._pintaAcciones }));
			}
			var check = [];
			if (this.props.TIPO == "multicheck") {
				check.push(React.createElement(tableFace.Column, {
					key: 'multicheck',
					label: '',
					width: 70,
					dataKey: 'selected',
					align: 'center',
					cellRenderer: function (checked) {
						return React.createElement('input', { type: 'checkbox', value: 'valor', checked: checked });
					} }));
			}

			var buscador = [];
			if (this.props.TIPO == "search") {
				buscador.push(React.createElement(
					'div',
					{ className: 'buscador', key: 'buscar' },
					React.createElement(
						'label',
						{ className: 'col-sm', htmlFor: this.props.modelkey },
						'Buscar'
					),
					React.createElement(
						'div',
						{ className: 'col-sm' },
						React.createElement('input', {
							type: 'text',
							className: 'form-control',
							name: 'buscador',
							ref: 'buscador',
							id: 'buscador',
							onKeyPress: this._onKeyPress })
					)
				));
			}

			var paginacion = '';

			if (this.props.PAG) {
				var elementos = this.state.DATA.length;
				var pagina = parseInt(this.state.PO / this.state.PL) || 0;
				var po = pagina == 0 ? 1 : pagina;
				var totalpags = this.state.COUNT == null ? 0 : parseInt(this.state.COUNT / this.state.PL) + 1 || 1;
				var auxCont = this.state.COUNT == null ? 'de ...' : ' de ' + totalpags.toString();
				paginacion = React.createElement(
					'ul',
					{ className: 'paginacion' },
					React.createElement(
						'li',
						null,
						React.createElement(
							'a',
							{ onClick: this.ONPREVIOUSPAGE },
							'«'
						),
						' ',
						po,
						' ',
						auxCont,
						' ',
						React.createElement(
							'a',
							{ onClick: this.ONNEXTPAGE },
							'»'
						)
					)
				);
			};
			//var controlledScrolling = this.props.left !== undefined || this.props.top !== undefined;
			var controlledScrolling = true;
			var eliminar = "";
			if (this.state.modalselected) {
				eliminar = this.state.modalselected.descripcion || this.state.modalselected.nombrecliente;
			}

			return React.createElement(
				'div',
				{ className: 'tablegrid' },
				React.createElement('div', { className: 'fondogris' }),
				React.createElement(
					'div',
					{ className: 'modalContent' },
					React.createElement(
						'div',
						{ className: 'modalCuerpo' },
						'La línea ',
						React.createElement(
							'b',
							null,
							eliminar
						),
						' sera eliminada'
					),
					React.createElement(
						'div',
						{ className: 'modalPie' },
						React.createElement(
							'a',
							{ href: 'javascript:void(0)', className: 'btn btn-primary btn-submit modal-btn', onClick: this._onDelete },
							'Confirmar'
						),
						React.createElement(
							'a',
							{ href: 'javascript:void(0)', className: 'btn btn-primary btn-submit modal-btn', onClick: this.cierraModal },
							'Cancelar'
						)
					)
				),
				buscador,
				React.createElement(
					'div',
					{ className: 'overGrid' },
					React.createElement(
						tableFace.Table,
						{ rowHeight: 80,
							rowHeightGetter: this._rowHeightGetter,
							className: 'algo',
							rowGetter: this._rowGetter,
							rowsCount: this.state.DATA.length,
							width: width,
							maxHeight: 5000,
							headerHeight: 50,
							onContentHeightChange: this._onContentHeightChange,
							rowClassNameGetter: this._rowClassNameGetter,
							scrollTop: this.props.top,
							scrollLeft: this.props.left,
							overflowX: controlledScrolling ? "hidden" : "auto",
							overflowY: controlledScrolling ? "auto" : "auto",
							onRowClick: this._onRowClick,
							onRowDoubleClick: this._handleDblClick },
						check,
						micolumns
					)
				),
				paginacion
			);
		}
	};

	var tableGridStore = _.extend(_.clone(tableGridBase), {
		mixins: [comp_mixins.getStateLE],
		propTypes: {
			COLUMNS: React.PropTypes.array.isRequired,
			SELECT: React.PropTypes.number.isRequired,
			PAG: React.PropTypes.bool,
			HEIGHT: React.PropTypes.string
		},
		ONCHECK: function ONCHECK(row, isSelected, selecteds) {
			this.lanzarAccion("selected", selecteds);
		},
		ONSELECTED: function ONSELECTED(row) {
			this.lanzarAccion("selected", row.pk);
			this.lanzarAccion("dataSelected", row);
		},
		ONDBLCLICK: function ONDBLCLICK(row) {
			this.lanzarAccion("dblclick", [row.pk]);
		},
		ONSELECTEDALL: function ONSELECTEDALL(event) {
			this.lanzarAccion("selected", []);
		},
		ONMASDATOS: function ONMASDATOS(event) {
			if (this.state.HASMORE) {
				this.lanzarAccion('masData', {});
			};
		},
		ONPAGINACION: function ONPAGINACION(event) {
			if (this.state.HASMORE) {
				this.lanzarAccion('paginacion', {});
			};
		},
		ONNEXTPAGE: function ONNEXTPAGE(event) {
			if (this.state.HASMORE) {
				this.lanzarAccion('paginacion', 'next');
			};
		},
		ONPREVIOUSPAGE: function ONPREVIOUSPAGE(event) {
			this.lanzarAccion('paginacion', 'previous');
		},
		ONCOUNT: function ONCOUNT(event) {
			if (this.state.HASMORE) {
				this.lanzarAccion('count', {});
			};
		}
	});

	var TableGrid1 = React.createClass(tableGridBase);
	var TableGrid2 = React.createClass(tableGridStore);

	function _getRelatedFromSCHEMALAYOUT(data, layout, aplic) {
		var url = URLResolver.getRESTQuery(aplic, layout.prefix, '');
		var midata = {};
		midata['s_' + layout.fk + "__exact"] = data[layout.fk];
		midata['fs_' + layout.ffield] = true;
		var aux = helpers.resquestGETre(url, midata, function (articulo) {
			return articulo.data[0][layout.ffield];
		}, function (xhr, textStatus, errorThrown) {
			console.log(xhr.responseText);
		});
		return aux.responseJSON.data[0][layout.ffield];
		//return 2;	
	};
	function _getColumnsFromSCHEMALAYOUT(SCHEMA, LAYOUT, IDENT) {
		//TODO, AQUI ES DONDE TIENE QUE DEDUCIRSE A PARTIR DE LAYOUT Y SCHEMA mas propiedades, formateo...
		var micolumns = [];
		var align;
		var obj;
		if (Object.keys(LAYOUT).length > 0) {
			LAYOUT = LAYOUT["fields"];
		}
		var fieldFlag = false;
		_.each(SCHEMA, function (obj2, keym) {
			if (SCHEMA[keym].field) {
				fieldFlag = true;
				return false;
			}
		});

		var objSchema = Object.keys(LAYOUT).length > 0 ? LAYOUT : SCHEMA;
		var tableWidth = 0;
		_.each(objSchema, function (obj2, keym) {
			if (keym.lastIndexOf("fk_", 0) === 0) {
				var key = keym.split("_")[1];
				var tipodato = {};
				tipodato['tipo'] = LAYOUT[keym].tipo ? LAYOUT[keym].tipo : 0;
				tableWidth += LAYOUT[keym] ? LAYOUT[keym].width ? LAYOUT[keym].width : 100 : 100;
				var daFormato = formatter.fromJSONfunc(tipodato);
				obj = {
					dataKey: key,
					label: LAYOUT[keym] ? LAYOUT[keym].label ? LAYOUT[keym].label : key : key,
					align: align,
					flexGrow: LAYOUT[keym] ? LAYOUT[keym].flex ? LAYOUT[keym].flex : 1 : 1,
					width: LAYOUT[keym] ? LAYOUT[keym].width ? LAYOUT[keym].width : 100 : 100,
					cellClassName: LAYOUT[keym] ? LAYOUT[keym].clase ? LAYOUT[keym].clase : "" : "",
					cellRenderer: function cellRenderer(cellData, index, row) {
						return daFormato(_getRelatedFromSCHEMALAYOUT(row, LAYOUT[keym], IDENT.APLIC));
					}
				};
				micolumns.push(obj);
			} else {
				var visible = LAYOUT[keym] ? LAYOUT[keym].visible ? LAYOUT[keym].visible : SCHEMA[keym].visible : SCHEMA[keym].visible;
				//var visible = SCHEMA[keym].visible;
				var field = SCHEMA[keym].field;

				if (visible == "false") visible = false;
				if (visible == "true") visible = true;

				if (fieldFlag && field && visible || !fieldFlag && visible) {
					align = "left";
					if (SCHEMA[keym].tipo == 16 || SCHEMA[keym].tipo == 19 || SCHEMA[keym].tipo == 37) {
						align = "right";
					}

					var daFormato = formatter.fromJSONfunc(LAYOUT[keym] ? LAYOUT[keym].tipo ? LAYOUT[keym].tipo : SCHEMA[keym].tipo : SCHEMA[keym].tipo);
					/*if(SCHEMA[keym].verbose_name == 'Nombre' || SCHEMA[keym].verbose_name == 'Descripción'){
	    	classn="miniletra";
	    }
	    if(SCHEMA[keym].verbose_name == 'Nombre'){
	    	classn="mininombre";
	    }*/
					tableWidth += LAYOUT[keym] ? LAYOUT[keym].width ? LAYOUT[keym].width : 100 : 100;
					obj = {
						dataKey: keym,
						label: LAYOUT[keym] ? LAYOUT[keym].label ? LAYOUT[keym].label : SCHEMA[keym].verbose_name : SCHEMA[keym].verbose_name,
						align: align,
						flexGrow: LAYOUT[keym] ? LAYOUT[keym].flex ? LAYOUT[keym].flex : 1 : 1,
						width: LAYOUT[keym] ? LAYOUT[keym].width ? LAYOUT[keym].width : 100 : 100,
						cellClassName: LAYOUT[keym] ? LAYOUT[keym].clase ? LAYOUT[keym].clase : "" : "",
						cellRenderer: function cellRenderer(cellData) {
							return daFormato(cellData);
						}
					};
					micolumns.push(obj);
				}
			}
		});
		micolumns.push(tableWidth);
		return micolumns;
	};
	//Busca acciones en cache
	function _getAccionFromModel(acciones, PREFIX) {
		var arrAcciones = [];
		for (var ac in acciones) {
			arrAcciones.push(YBAccionList.dameAccion(ac, PREFIX));
		}
		//list = YBAccionList.dameList(SCHEMA.option_list.aplic, SCHEMA.option_list.model);
		return arrAcciones;
	};
	//Busca Layout en cache
	function _getLayoutFromCache(APLIC, PREFIX, template) {
		var layout = {};
		layout = YBLayoutList.dameGridLayout(APLIC, PREFIX, template);
		return layout;
	};
	/* Renderiza una tabla con las opciones pasadas
	*/
	module.exports.generaTableGrid1Store = function (domObj, SCHEMA, LAYOUT, opts, IDENT, aux) {
		var obj = opts || {};
		obj = _.extend({ SELECT: 1,
			PAG: false }, obj);
		var ident = JSON.parse(IDENT.MODIF);
		var acciones = false;
		var tipoTabla = false;
		var rowClass = null;
		var layout = _getLayoutFromCache(IDENT.APLIC, IDENT.PREFIX, null);
		if (layout.hasOwnProperty('actions')) {
			acciones = {};
			acciones = layout['actions'];
			acciones = _getAccionFromModel(acciones, IDENT.PREFIX);
		}
		if (layout.hasOwnProperty('meta')) {
			if (layout.meta.hasOwnProperty('search')) tipoTabla = "search";
			if (layout.meta.hasOwnProperty('rowClass')) rowClass = layout.meta.rowClass;
		}
		var micolumns = _getColumnsFromSCHEMALAYOUT(SCHEMA, layout, IDENT);
		var tableWidth = micolumns.pop();
		//var mirelated=_getRelatedFromSCHEMALAYOUT(SCHEMA,LAYOUT);
		if (ident) {
			if (ident['multicheck']) {
				tipoTabla = "multicheck";
			}
			if (ident['search']) {
				tipoTabla = "search";
			}
			if (ident['acciones'] && !acciones) {
				acciones = {};
				acciones = ident['acciones'];
				acciones = _getAccionFromModel(acciones, IDENT.PREFIX);
			}
			if (ident['pag']) {
				obj.PAG = true;
			}
		}
		//funciones a partir del Store
		return ReactDOM.render(React.createElement(TableGrid2, _extends({ IDENT: IDENT,
			WIDTH: tableWidth,
			COLUMNS: micolumns,
			TIPO: tipoTabla,
			ACCIONES: acciones,
			ID: aux,
			ROWCLASS: rowClass
		}, obj)), domObj);
	};
	/* WEBPACK VAR INJECTION */}.call(exports, __webpack_require__(4)))

/***/ },
