	    this.data = helpers.getAJAX(url);
	};

	YBOptionList.prototype.dameDesc = function (aplic, list, cod) {
	    //TODO Control inteligente por codigo (si no es string...
	    try {
	        return this.data[aplic][list][cod];
	    } catch (ex) {
	        console.log('COMPONENTE DE LISTA INEXISTENTE ' + cod + aplic + ':' + list);
	        return null;
	    }
	};

	YBOptionList.prototype.dameList = function (aplic, list) {
	    try {
	        return this.data[aplic][list];
	    } catch (ex) {
	        console.log('LISTA INEXISTENTE ' + aplic + ':' + list);
	        return {};
	    }
	};

	// export the Object

	module.exports = new YBOptionList();

